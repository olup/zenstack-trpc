{
  "name": "zenstack-trpc",
  "version": "0.1.9",
  "description": "Auto-generate type-safe tRPC routers from ZenStack V3 schemas",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "zenstack",
    "trpc",
    "typescript",
    "orm",
    "prisma",
    "typesafe",
    "api",
    "rpc",
    "router",
    "codegen"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/olup/zenstack-trpc"
  },
  "homepage": "https://github.com/olup/zenstack-trpc#readme",
  "bugs": {
    "url": "https://github.com/olup/zenstack-trpc/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@trpc/server": ">=11.0.0",
    "@trpc/react-query": ">=11.0.0",
    "@tanstack/react-query": ">=5.0.0",
    "@zenstackhq/orm": ">=3.0.0",
    "zod": ">=3.0.0 || >=4.0.0"
  },
  "peerDependenciesMeta": {
    "@trpc/react-query": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.16",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@zenstackhq/cli": "^3.1.0",
    "@zenstackhq/orm": "^3.1.0",
    "better-sqlite3": "^12.5.0",
    "kysely": "^0.28.9",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  }
}
